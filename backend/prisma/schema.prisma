generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          String   @id @default(cuid())
  title       String
  price       Int
  description String
  image       String
  category    Json
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
